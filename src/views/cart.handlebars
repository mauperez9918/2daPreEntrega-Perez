<article>
  <table>
    <tr>
      <th>title:</th>
      <th>description:</th>
      <th>price:</th>
      <th>thumbnail:</th>
      <th>code:</th>
      <th>stock:</th>
      <th>category:</th>
      <th>status:</th>
    </tr>
    <tbody>
      {{#each products}}
        <tr>
          <td>{{this.product.description}}</td>
          <td>{{this.product.price}} USD</td>
          <td>{{this.product.thumbnail}}</td>
          <td>{{this.product.code}}</td>
          <td>{{this.product.stock}}</td>
          <td>{{this.product.category}}</td>
          <td>{{this.product.status}}</td>
          <td>{{this.quantity}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</article>